<!doctype html>

<html>
  <head>
    <meta charset="utf-8">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="description" content="{% block description %} Instilla porta l'innovazione nella vita delle persone. {% endblock %}">
    <meta property="og:type" content="website">
    <meta property="og:title" content="{% block title %} App Zero {% endblock %}">
    <meta property="og:description" content="{{ block ('description') }}">

    <title>{{block ('title') }}</title>
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700' rel='stylesheet' type='text/css'>
    <link href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css' rel='stylesheet' type='text/css'>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">



    <!--Adding Media Queries and Canvas Support for IE8-->
    <!--[if lt IE 9]>
      <script src="js/respond.min.js"></script>
      <script src="js/excanvas.js"></script>
    <![endif]-->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js" type="text/javascript"></script>
    <script async src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script async type="text/javascript" src="/js/plugins/jquery.touchSwipe.min.js"></script>
    <script async type="text/javascript" src="/js/plugins/jquery.placeholder.js"></script>
    <script async type="text/javascript" src="/js/plugins/jquery.bxslider.min.js"></script>
    <script async type="text/javascript" src="/js/plugins/jquery.validate.min.js"></script>
    <script async type="text/javascript" src="/js/plugins/smoothscroll.js"></script>
  </head>

  <body class="space-top index">
    {% include 'common/header.twig' %}
    {% block content %}
    {% endblock %}
    {% include 'common/footer.twig' %}
  </body>
</html>
